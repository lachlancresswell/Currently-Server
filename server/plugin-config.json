{
  "MDNSPlugin": {
    "path": "../mdns-plugin",
    "enabled": true,
    "config": {
      "transmit": {
        "priority": 1,
        "display": true,
        "readableName": "Transmit",
        "type": "boolean",
        "value": true
      },
      "receive": {
        "priority": 1,
        "display": true,
        "readableName": "Receive",
        "type": "boolean",
        "value": true
      },
      "deviceName": {
        "priority": 1,
        "display": true,
        "readableName": "Device Name",
        "type": "string",
        "value": "Lachlan's Devicesdf"
