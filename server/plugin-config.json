{
  "MDNSPlugin": {
    "path": "../mdns-plugin",
    "enabled": true,
    "config": {
      "transmit": {
        "priority": 1,
        "display": true,
        "readableName": "Transmit",
        "type": "boolean",
        "value": true
      },
      "receive": {
        "priority": 1,
        "display": true,
        "readableName": "Receive",
        "type": "boolean",
        "value": true
      },
      "deviceName": {
        "priority": 1,
        "display": true,
        "readableName": "Device Name",
        "type": "string",
        "value": "Lachlan's Devicesdf"
      },
      "txDelay": {
        "priority": 1,
        "display": true,
        "readableName": "Transmit Delay",
        "type": "number",
        "value": 4996,
        "restart": "plugin"
      }
    }
  },
  "InfluxPlugin": {
    "path": "../influx-plugin",
    "enabled": true,
    "config": {
      "databasePort": {
        "priority": 1,
        "display": true,
        "readableName": "Database Port",
        "type": "number",
        "value": 8002
      },
      "databaseDomain": {
        "priority": 1,
        "display": true,
        "readableName": "Database Domain",
        "type": "string",
        "value": "localhost"
      },
      "rxDelay": {
        "priority": 1,
        "display": true,
        "readableName": "Request Delay",
        "type": "number",
        "value": 1000
      }
    }
  },
  "IPPlugin": {
    "path": "../ip-plugin",
    "enabled": true,
    "config": {
      "ipaddress": {
        "priority": 1,
        "display": true,
        "readableName": "IP Address",
        "type": "ipaddress"
      },
      "gateway": {
        "priority": 1,
        "display": true,
        "readableName": "Gateway",
        "type": "ipaddress"
      },
      "prefix": {
        "priority": 1,
        "display": true,
        "readableName": "Network Prefix",
        "type": "number"
      },
      "dhcp": {
        "priority": 1,
        "display": false,
        "readableName": "DHCP",
        "type": "boolean"
      },
      "dns": {
        "priority": 1,
        "display": true,
        "readableName": "DNS Servers",
        "type": "strings"
      },
      "iface": {
        "priority": 1,
        "display": true,
        "readableName": "Network Interface",
        "type": "string",
        "value": "enp0s2"
      },
      "filePath": {
        "priority": 1,
        "display": true,
        "readableName": "Systemd Config Filepath",
        "type": "string",
        "value": "/etc/systemd/network/20-wired.network"
      }
    }
  }
}